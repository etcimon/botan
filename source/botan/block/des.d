/**
* DES
*/
class DES : BlockCipherFixedParams!(8, 8), BlockCipher, SymmetricAlgorithm
{
public:

    /*
    * DES Encryption
    */
    override void encryptN(const(ubyte)* input, ubyte* output, size_t blocks)
    {
        foreach (size_t i; 0 .. blocks)
        {
            ulong T =   (DES_IPTAB1[input[0]]     ) | (DES_IPTAB1[input[1]] << 1) |
                        (DES_IPTAB1[input[2]] << 2) | (DES_IPTAB1[input[3]] << 3) |
                        (DES_IPTAB1[input[4]] << 4) | (DES_IPTAB1[input[5]] << 5) |
                        (DES_IPTAB1[input[6]] << 6) | (DES_IPTAB2[input[7]]      );
            
            uint L = cast(uint)(T >> 32);
            uint R = cast(uint)(T);
            
            des_encrypt(L, R, *cast(uint[32]*) m_round_key.ptr);
            
            T =     (DES_FPTAB1[get_byte(0, L)] << 5) | (DES_FPTAB1[get_byte(1, L)] << 3) |
                    (DES_FPTAB1[get_byte(2, L)] << 1) | (DES_FPTAB2[get_byte(3, L)] << 1) |
                    (DES_FPTAB1[get_byte(0, R)] << 4) | (DES_FPTAB1[get_byte(1, R)] << 2) |
                    (DES_FPTAB1[get_byte(2, R)]     ) | (DES_FPTAB2[get_byte(3, R)]      );
            T = rotateLeft(T, 32);
            
            storeBigEndian(T, output);
            
            input += BLOCK_SIZE;
            output += BLOCK_SIZE;
        }
    }


    /*
    * DES Decryption
    */
    override void decryptN(const(ubyte)* input, ubyte* output, size_t blocks)
    {
        foreach (size_t i; 0 .. blocks)
        {
            ulong T =     (DES_IPTAB1[input[0]]   ) | (DES_IPTAB1[input[1]] << 1) |
                        (DES_IPTAB1[input[2]] << 2) | (DES_IPTAB1[input[3]] << 3) |
                        (DES_IPTAB1[input[4]] << 4) | (DES_IPTAB1[input[5]] << 5) |
                        (DES_IPTAB1[input[6]] << 6) | (DES_IPTAB2[input[7]]      );
            
            uint L = cast(uint)(T >> 32);
            uint R = cast(uint)(T);
            
            des_decrypt(L, R, *cast(uint[32]*) m_round_key.ptr);
            
            T =     (DES_FPTAB1[get_byte(0, L)] << 5) | (DES_FPTAB1[get_byte(1, L)] << 3) |
                    (DES_FPTAB1[get_byte(2, L)] << 1) | (DES_FPTAB2[get_byte(3, L)] << 1) |
                    (DES_FPTAB1[get_byte(0, R)] << 4) | (DES_FPTAB1[get_byte(1, R)] << 2) |
                    (DES_FPTAB1[get_byte(2, R)]     ) | (DES_FPTAB2[get_byte(3, R)]      );
            
            T = rotateLeft(T, 32);
            
            storeBigEndian(T, output);
            
            input += BLOCK_SIZE;
            output += BLOCK_SIZE;
        }
    }

    override void clear()
    {
        zap(m_round_key);
    }

    @property string name() const { return "DES"; }
    override @property size_t parallelism() const { return 1; }
    override BlockCipher clone() const { return new DES; }
    override size_t blockSize() const { return super.blockSize(); }
    override KeyLengthSpecification keySpec() const { return super.keySpec(); }
protected:
    /*
    * DES Key Schedule
    */
    override void keySchedule(const(ubyte)* key, size_t)
    {
        m_round_key.resize(32);
        des_key_schedule(*cast(uint[32]*) m_round_key.ptr, *cast(ubyte[8]*) key);
    }

    SecureVector!uint m_round_key;
}

/**
* Triple DES
*/
class TripleDES : BlockCipherFixedParams!(8, 16, 24, 8), BlockCipher, SymmetricAlgorithm
{
public:
    /*
    * TripleDES Encryption
    */
    override void encryptN(const(ubyte)* input, ubyte* output, size_t blocks)
    {
        foreach (size_t i; 0 .. blocks)
        {
            ulong T =   (DES_IPTAB1[input[0]]     ) | (DES_IPTAB1[input[1]] << 1) |
                        (DES_IPTAB1[input[2]] << 2) | (DES_IPTAB1[input[3]] << 3) |
                        (DES_IPTAB1[input[4]] << 4) | (DES_IPTAB1[input[5]] << 5) |
                        (DES_IPTAB1[input[6]] << 6) | (DES_IPTAB2[input[7]]      );
            
            uint L = cast(uint)(T >> 32);
            uint R = cast(uint)(T);

            des_encrypt(L, R, *cast(uint[32]*) m_round_key.ptr);
            des_decrypt(R, L, *cast(uint[32]*) &m_round_key[32]);
            des_encrypt(L, R, *cast(uint[32]*) &m_round_key[64]);
            
            T =     (DES_FPTAB1[get_byte(0, L)] << 5) | (DES_FPTAB1[get_byte(1, L)] << 3) |
                    (DES_FPTAB1[get_byte(2, L)] << 1) | (DES_FPTAB2[get_byte(3, L)] << 1) |
                    (DES_FPTAB1[get_byte(0, R)] << 4) | (DES_FPTAB1[get_byte(1, R)] << 2) |
                    (DES_FPTAB1[get_byte(2, R)]     ) | (DES_FPTAB2[get_byte(3, R)]     );
            
            T = rotateLeft(T, 32);
            
            storeBigEndian(T, output);
            
            input += BLOCK_SIZE;
            output += BLOCK_SIZE;
        }
    }
    /*
    * TripleDES Decryption
    */
    override void decryptN(const(ubyte)* input, ubyte* output, size_t blocks)
    {
        foreach (size_t i; 0 .. blocks)
        {
            ulong T =   (DES_IPTAB1[input[0]]     ) | (DES_IPTAB1[input[1]] << 1) |
                        (DES_IPTAB1[input[2]] << 2) | (DES_IPTAB1[input[3]] << 3) |
                        (DES_IPTAB1[input[4]] << 4) | (DES_IPTAB1[input[5]] << 5) |
                        (DES_IPTAB1[input[6]] << 6) | (DES_IPTAB2[input[7]]     );
            
            uint L = cast(uint)(T >> 32);
            uint R = cast(uint)(T);

            des_decrypt(L, R, *cast(uint[32]*) &m_round_key[64]);
            des_encrypt(R, L, *cast(uint[32]*) &m_round_key[32]);
            des_decrypt(L, R, *cast(uint[32]*) m_round_key.ptr);
            
            T =     (DES_FPTAB1[get_byte(0, L)] << 5) | (DES_FPTAB1[get_byte(1, L)] << 3) |
                    (DES_FPTAB1[get_byte(2, L)] << 1) | (DES_FPTAB2[get_byte(3, L)] << 1) |
                    (DES_FPTAB1[get_byte(0, R)] << 4) | (DES_FPTAB1[get_byte(1, R)] << 2) |
                    (DES_FPTAB1[get_byte(2, R)]     ) | (DES_FPTAB2[get_byte(3, R)]     );
            
            T = rotateLeft(T, 32);
            
            storeBigEndian(T, output);
            
            input += BLOCK_SIZE;
            output += BLOCK_SIZE;
        }
    }

    override void clear()
    {
        zap(m_round_key);
    }

    @property string name() const { return "TripleDES"; }
    override @property size_t parallelism() const { return 1; }
    override BlockCipher clone() const { return new TripleDES; }
    override size_t blockSize() const { return super.blockSize(); }
    override KeyLengthSpecification keySpec() const { return super.keySpec(); }
protected:
    /*
    * TripleDES Key Schedule
    */
    override void keySchedule(const(ubyte)* key, size_t length)
    {
        m_round_key.resize(3*32);
        des_key_schedule(*cast(uint[32]*)m_round_key.ptr, key[0 .. 8]);
        des_key_schedule(*cast(uint[32]*)&m_round_key[32], key[8 .. 16]);
        
        if (length == 24) {
            des_key_schedule(*cast(uint[32]*)&m_round_key[64], key[16 .. 24]);
        }
        else {
            copyMem(&m_round_key[64], m_round_key.ptr, 32);
        }
    }

    SecureVector!uint m_round_key;
}

